{
  "name": "developers-am",
  "version": "1.0.0",
  "description": "Armenian Developers Association website",
  "author": "Suren Samarchyan",
  "license": "MIT",
  "keywords": [
    "developers-am"
  ],
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --trailing-comma --no-semi --single-quote --write \"src/**/*.js\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest --passWithNoTests",
    "lint": "yarn lint:tsc && yarn lint:eslint && yarn lint:stylelint",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx src",
    "lint:stylelint": "stylelint \"src/**/*.tsx\"",
    "prettier": "npx prettier --write \"*/.{js,json,md,ts,tsx}\"",
    "prettier:debug": "npx prettier --loglevel=debug --write \"*/.{js,json,md,ts,tsx}\"",
    "prettier:check": "npx prettier --loglevel=debug --check \"*/.{js,json,md,ts,tsx}\""
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.53",
    "gatsby-cli": "^2.12.91",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-netlify": "^2.3.4",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-source-datocms": "^2.4.0",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "graphql": "14.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "gatsby-plugin-sass": "^2.1.24",
    "node-sass": "^4.13.0",
    "react-masonry-component": "^6.2.1",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "prettier": "^1.17.0"
  }
}
